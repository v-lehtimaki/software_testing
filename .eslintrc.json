{
    "env": {
        "node": true,
        "es2020": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module",
        "impliedStrict": true
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prefer-destructuring": "off",
        "func-names": "off",
        "no-unused-expressions": "off",
        "import/extensions": "off"
    }
}